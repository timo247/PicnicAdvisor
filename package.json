{
  "name": "shroomshare",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "DEBUG='api:succes api:errors' nodemon",
    "start": "node ./src/bin/start",
    "stopdb": "brew services stop mongodb-community@6.0",
    "seed": "node ./src/seeders/seeders.js",
    "seed-test": "cross-env DATABASE_URL=mongodb://127.0.0.1:27017/test node ./src/seeders/seeders.js",
    "test": "cross-env DATABASE_URL=mongodb://127.0.0.1:27017/test node --experimental-vm-modules node_modules/.bin/jest --coverage --runInBand",
    "doc": "npx swagger-jsdoc-generator swagger.json > collection.json"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "debug": "^4.3.4",
    "express": "~4.18.1",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.10.0",
    "mongoose": "^6.6.5",
    "morgan": "~1.10.0",
    "swagger-ui-express": "^4.5.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.2.1",
    "jest-extended": "^3.1.0",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.0",
    "swagger-jsdoc": "^6.2.5",
    "swagger-jsdoc-generator": "^1.0.3"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ]
  }
}